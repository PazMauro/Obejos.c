/*

6-
#include <stdio.h>
int main(){
	
	int sueldo, b=0;
	float alquiler;
	
	printf("Ingrese cuanto cobra: ");
	scanf("%d", & sueldo);
	if(sueldo<=202){
		b=0;
	}
	else if(202<sueldo && sueldo<=607){
		b=5;
	}
	else if(607<sueldo && sueldo<=1013){
		b=10;
	}
	else if(1013<sueldo && sueldo<=1418){
		b=15;
	}
	else if(1418<sueldo){
		b=25;
	}
	alquiler=(sueldo*b)/100;
	printf("El precio del alquiler es %f", alquiler);
}

*/

