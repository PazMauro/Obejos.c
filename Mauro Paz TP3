	scanf("%d", & minutos);
	
	if(hora>=1 && hora<=12){
		printf("%d:%d AM",hora,minutos);
	}
	
	if(hora>=13 && hora<=24){
		printf("%d:%d PM",hora, minutos);
	}
	
	
}
