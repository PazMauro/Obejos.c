/*
5-
#include <stdio.h>
int main(){
	int nota;
	
	printf("Ingrese la nota que se saco: ");
	scanf("%d", & nota);
	
	if(nota>=6 && nota<=7){
		printf("Aprobado");
	}
	else if(nota>=2 && nota<=5){
		printf("Reprobado");
	}
	else if(nota>=8 && nota<=10){
		printf("aprobaste, felicitaciones");
	}
	else if(nota==1){
		printf("reprobaste, Debes esforzarte mas");
	}
	else if(nota>10){
		printf("Estas ingresando mal los numeros");
	}
}
